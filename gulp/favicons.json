{
  "appName": "Spotify Premium Playlists",
  "appShortName": "SPP",

  "path": "/favicons/",

  "background": "#000000",
  "theme_color": "#000000",
  "appleStatusBarStyle": "black-translucent",

  "scope": "/",
  "start_url": "/",

  "display": "standalone",
  "orientation": "portrait",

  "logging": false,
  "html": "icons.html",
  "pipeHTML": true,
  "replace": true,

  "icons": {
    "android": {
      "background": "#000000",
      "offset": 5
    },
    "appleIcon": {
      "offset": 5
    },
    "appleStartup": {
      "offset": 10
    },
    "coast": false,
    "favicons": {
      "background": "#000000",
      "offset": 5
    },
    "firefox": {
      "offset": 10,
      "overlayGlow": false
    },
    "windows": {
      "background": "#000000",
      "offset": 5
    },
    "yandex": false
  }
}
